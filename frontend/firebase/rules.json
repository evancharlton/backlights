{
  "rules": {
    ".read": false,
    ".write": false,
    "rgba": {
      ".read": false,
      ".write": false,
      "left": {
        ".read": true,
        ".write": true,
        ".validate": "newData.isString() && newData.val().matches(/^[0-9A-Fa-f]{8}$/)"
      },
      "right": {
        ".read": true,
        ".write": true,
        ".validate": "newData.isString() && newData.val().matches(/^[0-9A-Fa-f]{8}$/)"
      }
    }
  }
}
